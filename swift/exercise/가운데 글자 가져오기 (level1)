func solution(_ s:String) -> String {
    
    let len = s.count
    let i = len / 2
    
    var str = ""
    
    if (len % 2 == 0){
        str = "\(s[s.index(s.startIndex, offsetBy: i-1)])"
            + "\(s[s.index(s.startIndex, offsetBy: i)])"
        return str
    } else {
        str = "\(s[s.index(s.startIndex, offsetBy: i)])"
        return str
    }
    
}
